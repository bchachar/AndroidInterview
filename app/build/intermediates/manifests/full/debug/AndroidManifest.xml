<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bchachar.androidinterview"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.bchachar.androidinterview.FrontPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.bchachar.androidinterview.SimpleQuestions"
            android:label="@string/title_activity_simple_questions" >
        </activity>
        <activity
            android:name="com.bchachar.androidinterview.ToughQuestions"
            android:label="@string/title_activity_tough_questions" >
        </activity>
    </application>

</manifest>